@model IList<PlaylistViewModel>
@{
    ViewData["Title"] = "View";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken();


    @if (Model.Count() == 0)
    {
        <p>There are no playlists</p>
    }

    else
    {
        @foreach (var item in Model)
        {
            <p>@item.Title | @item.Duration</p>
        }
    }
}